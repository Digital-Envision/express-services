{
  "name": "chat-service",
  "version": "1.0.0",
  "description": "Boilerplate for user chat services (e.g private/direct chat, group chat, etc)",
  "scripts": {
    "migrate": "tsx cli/migrator.ts migrate --type migration",
    "migrate:create": "tsx cli/migrator.ts create --type migration --output migrations",
    "migrate:up": "pnpm run migrate --action up",
    "migrate:down": "pnpm run migrate --action down",
    "migrate:to": "pnpm run migrate --action to",
    "migrate:revert": "pnpm run migrate --action revert",
    "migrate:latest": "pnpm run migrate --action latest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Muhammad Firdaus Sati",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "mysql2": "^3.9.1",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.4",
    "umzug": "^3.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/sequelize": "^4.28.20",
    "database-cli": "file:../../packages/database-cli"
  }
}
